{
  "name": "@sarahG/cv-vuejs",
  "version": "0.0.1",
  "description": "Cv en ligne avec vue.js",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint --fix --cache src",
    "build": "npx parcel build src/index.html",
