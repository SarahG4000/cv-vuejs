  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SarahG4000/cv-vuejs/issues"
  },
  "homepage": "https://github.com/SarahG4000/cv-vuejs#readme",
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@becode/eslint-config": "^0.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-unicorn": "^20.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.json": [
      "npx prettier --write"
    ],
    "*.js": [
      "npx eslint --fix --cache"
    ]
